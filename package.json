    "@tanstack/react-router": "^1.30.1",
    "@tanstack/router-vite-plugin": "^1.30.0",
    "ably": "^2.0.3",
    "ahooks": "^3.7.11",
    "axios": "^1.6.8",
    "bootstrap": "^5.3.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "express": "^4.19.2",
    "immer": "^10.0.4",
    "matchers": "^0.0.1",
    "mongodb": "^6.5.0",
